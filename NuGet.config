<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <!-- If you have a private feed, add it here:
    <add key="MyPrivateFeed" value="https://yourprivatefeed.url/nuget/v3/index.json" /> -->
  </packageSources>
  <!-- Optional: Package source mapping ensures packages come from the intended source -->
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
